{
    "name": "yonis-savary/cube",
    "description": "Simple Web Framework for PHP8+",
    "keywords": ["php", "web", "framework", "backend"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cube\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cube\\Tests\\Units\\": "tests/units",
            "Cube\\Tests\\Integration\\": "tests/integration"
        }
    },
    "authors": [
        {
            "name": "yonis-savary",
            "email": "yonissavary@outlook.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "psr/log": "^3.0.2",
        "symfony/process": "7.2.0",
        "cboden/ratchet": "^0.4.4",
        "react/http": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "11.5.3",
        "friendsofphp/php-cs-fixer": "v3.70.0"
    }
}
